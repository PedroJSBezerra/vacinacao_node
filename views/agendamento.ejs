<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style media="screen">
        *,*::before,*::after{
            box-sizing: border-box;
            margin: 0;
            padding: 0;
        }
        body{
            font-family: sans-seriff;
        }
        .cadastro{
            margin: 0 10%;
            display: flex;
            flex-direction: column;
        }
        table{
            width: 100%;
        }
        td{
            text-align: center;
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <div class="app">
        <header>
            <h3>Agendamento</h3>
        </header>
        <main>
            <table>
                <thead>
                    <td>id</td>
                    <td>nome</td>
                    <td>cpf</td>
                    <td>idade</td>
                    <td>comorbidade</td>
                    <td>endereco</td>
                </thead>
                <% foo.forEach(function(ppl) { %>
                    <tr>
                        <td><%= ppl.id%></td>
                        <td><%= ppl.nome%></td>
                        <td><%= ppl.cpf%></td>
                        <td><%= ppl.idade%></td>
                        <td><%= ppl.comorbidade%></td>
                        <td><%= ppl.endereco%></td>
                    </tr>
                <% }) %>
            </table>
        </main>
        <footer>

        </footer>
    </div>
    <script>
    </script>
</body>
</html>
